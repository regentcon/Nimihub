local player = game.Players.LocalPlayer

-- ì•ˆì „í•˜ê²Œ Info ì¸ìŠ¤í„´ìŠ¤ ì°¾ê¸°
local info = player:FindFirstChild("Info", true)
if info and info.Parent then
	local tool = info.Parent
	local char = player.Character
	local humanoid = char and char:FindFirstChildOfClass("Humanoid")

	if humanoid and tool:IsA("Tool") then
		repeat
			local success, err = pcall(function()
				humanoid:EquipTool(tool)
			end)
			if not success then
			end
			wait(0.1)
		until char and char:FindFirstChild(tool.Name)
		KeyTable = {}
		retrieved = filtergc("table", { Keys = { "remote_key" }, }, true)
		if retrieved then
			KeyTable = retrieved.remote_key
		end

		remote_key = {}
	elseif  player.Character:FindFirstChild("Info",true).Parent then
		return true
	end
else
	print("ì´ì„ ì°¾ì„ ìˆ˜ ì—†ìŠµë‹ˆë‹¤.")
	return false
end

PlaceId, JobId = game.PlaceId, game.JobId
local Players = game.Players
local TeleportService = game:GetService("TeleportService")

for i, plr in pairs(game.Players:GetPlayers()) do
	plr.Chatted:Connect(function(msg)
		local command, targetName = msg:match("^[:;](%S+)%s+(%S+)")
		if command and (command == "ì˜ì°½" or command == "arrest") then
			for i, target in pairs(game.Players:GetPlayers()) do
				if string.find(target.Name:lower(), targetName:lower(), 1, true) then
					if #Players:GetPlayers() <= 1 then
						Players.LocalPlayer:Kick("\nRejoining...")
						wait()
						TeleportService:Teleport(PlaceId, Players.LocalPlayer)
					else
						TeleportService:TeleportToPlaceInstance(PlaceId, JobId, Players.LocalPlayer)
					end
				end
			end
		end
	end)
end

game.Players.PlayerAdded:Connect(function(plr)
	plr.Chatted:Connect(function(msg)
		local command, targetName = msg:match("^:(%S+)%s+(%S+)")
		if command and (command == "ì˜ì°½" or command == "arrest") then
			for i, target in pairs(game.Players:GetPlayers()) do
				if string.find(target.Name:lower(), targetName:lower(), 1, true) then
					if #Players:GetPlayers() <= 1 then
						Players.LocalPlayer:Kick("\nRejoining...")
						wait()
						TeleportService:Teleport(PlaceId, Players.LocalPlayer)
					else
						TeleportService:TeleportToPlaceInstance(PlaceId, JobId, Players.LocalPlayer)
					end
				end
			end
		end
	end)
end)

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Players = game:GetService("Players")

local DamageRemote = ReplicatedStorage:WaitForChild("Assets"):WaitForChild("Misc")
	:WaitForChild("CWS_Directory"):WaitForChild("remotes"):WaitForChild("Damage")

function Notification(Title,text)
	local CoreGui = game:GetService("StarterGui")
	CoreGui:SetCore("SendNotification", {
		Title = Title;
		Text = text;
	})
end

Notification("ìž‘ë™ë²•","íˆ´ì„ ìž¥ì°©í•˜ê³  ë²„íŠ¼ì„ í´ë¦­í•˜ì—¬ ì‚¬ëžŒë“¤ì„ ì£½ì´ì„¸ìš”")



local function ReplaceChar(pos, str, r)
	return str:sub(1, pos-1) .. r .. str:sub(pos+1)
end

local function GetPlayerUserId(Player)
	local user_id = Player.UserId
	if #tostring(user_id) ~= 9 then
		user_id = math.abs(user_id * 100000000)
	end
	return user_id
end

local function NextRemoteKey(Player)
	local user_id = GetPlayerUserId(Player)
	local key = remote_key[user_id]
	if not key then return nil end

	local a = key:sub(4,4)
	local p = string.byte(key:sub(1,1))
	local s = tonumber(a) + tonumber(a)
	if s > 9 then s = 1 end
	local n = key:gsub(a, tostring(s))
	local h = p + 4 - s
	if h > 90 then h = 65 end
	local n1 = ReplaceChar(1, n, string.char(h))
	remote_key[user_id] = n1

	return n1
end

local userid = GetPlayerUserId(player)

do
	local initial
	for _, v in pairs(KeyTable) do
		initial = v
		break
	end
	remote_key[userid] = initial
end

local function getTool()
	for i, v in pairs(game.Players:GetPlayers()) do
		if v ==game.Players.LocalPlayer and v.Character:FindFirstChild("Info",true).Parent then
			return v.Character:FindFirstChild("Info",true).Parent
		end
	end
end

local function isTargetValid(targetPlayer)
	if not targetPlayer or targetPlayer == player then return false end
	local char = targetPlayer.Character
	if not char or not char.Parent then return false end
	local humanoid = char:FindFirstChildOfClass("Humanoid")
	if not humanoid or humanoid.Health <= 0 then return false end
	return true
end

local function ByPassKey(k, targetPlayer)
	if not k then return end
	if not isTargetValid(targetPlayer) then return end

	local humanoid = targetPlayer.Character:FindFirstChild("Humanoid")
	local tool = getTool()
	if not tool then return end

	local Info
	local ok, result = pcall(require, tool.Info)
	if ok then
		Info = result
	else
		return
	end

	local args = {
		[1] = {
			["damage"] = Info.damage.headshot,
			["k"] = k,
			["hit_name"] = "Head",
			["target"] = humanoid,
			["velocity"] = Vector3.new(0,0,0),
			["tool"] = tool,
			["auth"] = false
		}
	}

	pcall(function()
		DamageRemote:FireServer(unpack(args))
	end)
end

local queue = {}
local processing = false
local debounce = false

local PROCESS_INTERVAL = 0.08
local BURST_SIZE = 3

local function enqueueTargets()
	table.clear(queue)
	for _, v in pairs(Players:GetPlayers()) do
		if v ~= player and isTargetValid(v) then
			table.insert(queue, v)
		end
	end
end

local function processQueue()
	if processing then return end
	processing = true
	debounce = true

	task.spawn(function()
		local idx = 1
		while idx <= #queue do
			local processedThisTick = 0

			while processedThisTick < BURST_SIZE and idx <= #queue do
				local target = queue[idx]

				if isTargetValid(target) then
					local k = NextRemoteKey(player)
					if k then
						ByPassKey(k, target)
					end
				end

				idx += 1
				processedThisTick += 1
			end
		end

		processing = false
		debounce = false
	end)
end

TextButton.MouseButton1Click:Connect(function()
	for i=1, 60 do
		if debounce then
			return
		end

		if not getTool() then
			return
		end

		if not remote_key[userid] then
			return
		end

		enqueueTargets()

		if #queue > 0 then
			processQueue()
		end
	end
end)
   end, 
})    
    
