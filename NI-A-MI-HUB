local Button = Tab:CreateButton({
   Name = "ìƒëŒ€ë’¤ë¡œ í‹°í”¼",
   Callback = function()
-- ì„œë¹„ìŠ¤ ê°€ì ¸ì˜¤ê¸°
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local Workspace = game:GetService("Workspace")

local localPlayer = Players.LocalPlayer
local camera = Workspace.CurrentCamera
local targetPlayer = nil
local teleporting = false

-- ì‹œì  ê³ ì •ìš© ë³€ìˆ˜
local originalCameraSubject = camera.CameraSubject

-- íƒ€ê²Ÿ í”Œë ˆì´ì–´ ì„ íƒ (ìžê¸° ìžì‹  ì œì™¸í•˜ê³  ì•„ë¬´ë‚˜)
local function pickTarget()
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= localPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            return player
        end
    end
    return nil
end

-- í…”ë ˆí¬íŠ¸ ë£¨í”„
RunService.RenderStepped:Connect(function()
    if teleporting and targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
        local targetHRP = targetPlayer.Character.HumanoidRootPart
        local myChar = localPlayer.Character
        if myChar and myChar:FindFirstChild("HumanoidRootPart") then
            local myHRP = myChar.HumanoidRootPart

            -- íƒ€ê²Ÿì˜ ë’¤ 4 studs ìœ„ì¹˜ + íšŒì „ê¹Œì§€ ë”°ë¼ê°
            local newCF = targetHRP.CFrame * CFrame.new(0, 0, 3.5)
            myHRP.CFrame = newCF
        end
    end
end)

-- ìž…ë ¥ ê°ì§€ (R í† ê¸€)
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == Enum.KeyCode.R then
        teleporting = not teleporting
        if teleporting then
            targetPlayer = pickTarget()
            if camera.CameraSubject then
                originalCameraSubject = camera.CameraSubject
            end
            -- ì¹´ë©”ë¼ í”ë“¤ë¦¼ ë°©ì§€: ê³ ì •ëœ ì‹œì  ìœ ì§€
            camera.CameraSubject = originalCameraSubject
            print("âœ… ë”°ë¼ê°€ê¸° ON (íšŒì „ í¬í•¨, ì‹œì  ê³ ì •)")
        else
            print("âŒ ë”°ë¼ê°€ê¸° OFF")
        end
    end
end)

end,
})


