local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Camera = workspace.CurrentCamera
local LocalPlayer = Players.LocalPlayer
local UserInputService = game:GetService("UserInputService")
local CoreGui = game:GetService("CoreGui")

local fovRadius = 100
local fovCircle = nil
local SilentAimRS = nil
local remote_key = {}
local CurrentTarget = nil
local processing = false

local function getSafeGuiParent()
	return CoreGui or LocalPlayer:FindFirstChildOfClass("PlayerGui")
end

local function removeExistingAimbotGuis()
	local parent = getSafeGuiParent()
	if parent then
		local oldGui = parent:FindFirstChild("MobileAimbotUI")
		if oldGui then oldGui:Destroy() end
	end
	if fovCircle then fovCircle:Destroy() fovCircle = nil end
end


local function applyESP(char)
	if not char then return end
	local h = char:FindFirstChild("TargetHighlight")
	if not h then
		h = Instance.new("Highlight")
		h.Name = "TargetHighlight"
		h.FillColor = Color3.fromRGB(255, 0, 0)
		h.OutlineColor = Color3.fromRGB(255, 255, 255)
		h.FillTransparency = 0.5
		h.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
		h.Parent = char
	end
end

local function removeESP(char)
	if char and char:FindFirstChild("TargetHighlight") then
		char.TargetHighlight:Destroy()
	end
end

local function removeAllESP()
	for _, p in pairs(Players:GetPlayers()) do
		if p.Character then removeESP(p.Character) end
	end
end

local function isVisible(targetPart)
	local origin = Camera.CFrame.Position
	local destination = targetPart.Position
	local direction = destination - origin
	local raycastParams = RaycastParams.new()
	raycastParams.FilterDescendantsInstances = {LocalPlayer.Character, Camera}
	raycastParams.FilterType = Enum.RaycastFilterType.Exclude
	local result = workspace:Raycast(origin, direction, raycastParams)
	if result then
		return result.Instance:IsDescendantOf(targetPart.Parent)
	end
	return true
end

local function ReplaceChar(pos, str, r)
	return str:sub(1, pos-1) .. r .. str:sub(pos+1)
end

local function GetPlayerUserId(Player)
	local user_id = Player.UserId
	if #tostring(user_id) ~= 9 then
		user_id = math.abs(user_id * 100000000)
	end
	return user_id
end

local function NextRemoteKey(Player)
	local user_id = GetPlayerUserId(Player)
	local key = remote_key[user_id]
	if not key then return nil end

	local a = key:sub(4,4)
	local p = string.byte(key:sub(1,1))
	local s = (tonumber(a) or 0) * 2
	if s > 9 then s = 1 end
	local n = key:gsub(a, tostring(s))
	local h = p + 4 - s
	if h > 90 then h = 65 end
	local n1 = ReplaceChar(1, n, string.char(h))
	remote_key[user_id] = n1
	return n1
end

local function isTargetValid(targetPlayer)
	if not targetPlayer or targetPlayer == LocalPlayer then return false end
	local char = targetPlayer.Character
	if not char or not char.Parent then return false end
	local humanoid = char:FindFirstChildOfClass("Humanoid")
	if not humanoid or humanoid.Health <= 0 then return false end
	return true
end

local function getTool()
	local tool = LocalPlayer.Character:FindFirstChild("Info", true) or LocalPlayer.Backpack:FindFirstChild("Info", true)
	return tool and tool.Parent or nil
end

local function ByPassKey(k, targetPlayer)
	if not k or not isTargetValid(targetPlayer) then return end
	local tool = getTool()
	if not tool then return end
	local ok, Info = pcall(require, tool.Info)
	if not ok then return end

	local damageRemote = ReplicatedStorage:FindFirstChild("Assets"):FindFirstChild("Misc"):FindFirstChild("CWS_Directory"):FindFirstChild("remotes"):FindFirstChild("Damage")
	if damageRemote then
		damageRemote:FireServer({
			["damage"] = Info.damage.headshot,
			["k"] = k,
			["hit_name"] = "Head",
			["target"] = targetPlayer.Character.Humanoid,
			["velocity"] = Vector3.new(0, 0, 0),
			["tool"] = tool,
			["auth"] = false
		})
	end
end

local Window = Fluent:CreateWindow({
	Title = "Cave Army",
	SubTitle = "By JHK",
	TabWidth = 160,
	Size = UDim2.fromOffset(500, 420),
})

local Tab = Window:AddTab({ Title = "Main", Icon = "user" })
local Misc = Window:AddTab({ Title = "Misc", Icon = "plus-circle" })
local Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })

local Section = Tab:AddSection("Silent Aim")
local KillAllSection = Tab:AddSection("Kill All")

local Slider = Section:AddSlider("Fov", {
	Title = "Fov", Default = 100, Min = 50, Max = 200, Rounding = 1,
	Callback = function(Value) fovRadius = Value end
})

local SilentAim = Section:AddToggle("SilentAim", { Title = "SilentAim", Default = false })
local ShowTargetPlayer = Section:AddToggle("ShowTargetPlayer", { Title = "ShowTargetPlayer", Default = false })

Section:AddButton({
	Title = "ë¬´í•œì´ì•Œ",
	Description = "ì´ ë“¤ì§€ë§ê³  ì‚¬ìš©í•˜ì„¸ìš”",
	Callback = function()
		task.spawn(function()
			while wait() do
				for i, v in pairs(game.Players.LocalPlayer.Backpack:GetDescendants()) do
					if v:IsA("Tool") and v:FindFirstChild("Info") then
						v:SetAttribute("ammo", math.huge)
					end
				end
			end
		end)
	end
	})

	SilentAim:OnChanged(function()
	if SilentAim.Value then
		removeExistingAimbotGuis()
		local screenGui = Instance.new("ScreenGui", getSafeGuiParent())
		screenGui.Name = "MobileAimbotUI"

		fovCircle = Instance.new("Frame", screenGui)
		fovCircle.AnchorPoint = Vector2.new(0.5, 0.5)
		fovCircle.BackgroundTransparency = 1
		local stroke = Instance.new("UIStroke", fovCircle)
		stroke.Color = Color3.new(0.33, 0.33, 1)
		stroke.Thickness = 2
		Instance.new("UICorner", fovCircle).CornerRadius = UDim.new(1, 0)

		SilentAimRS = RunService.RenderStepped:Connect(function()
			local mousePos = UserInputService:GetMouseLocation()
			if fovCircle then
				local currentRadius = tonumber(fovRadius) or 100
				fovCircle.Position = UDim2.new(0, mousePos.X, 0, mousePos.Y)
				fovCircle.Size = UDim2.new(0, currentRadius * 2, 0, currentRadius * 2)
			end

			local closestTarget = nil
			local lowestScore = math.huge 
			local radiusLimit = tonumber(fovRadius) or 100

			removeAllESP()

			local myRoot = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
			if not myRoot then return end

			for _, p in pairs(Players:GetPlayers()) do
				if p ~= LocalPlayer and p.Character and p.Character:FindFirstChild("Head") and not p.Character:FindFirstChild("Tag") and p.Character:FindFirstChild("Humanoid").Health > 0 then
					local char = p.Character
					local head = char.Head

					local screenPoint, onScreen = Camera:WorldToViewportPoint(head.Position)

					if onScreen then
						local mouseDistance = (Vector2.new(screenPoint.X, screenPoint.Y) - mousePos).Magnitude

						if mouseDistance <= radiusLimit then
							if isVisible(head) then
								local worldDistance = (head.Position - myRoot.Position).Magnitude
								local score = mouseDistance + (worldDistance * 0.8)

								if score < lowestScore then
									lowestScore = score
									closestTarget = head
								end
							end
						end
					end
				end
			end
			CurrentTarget = closestTarget
			if ShowTargetPlayer.Value and CurrentTarget and CurrentTarget.Parent then
				applyESP(CurrentTarget.Parent)
			end
		end)
	else
		if SilentAimRS then SilentAimRS:Disconnect() end
		removeExistingAimbotGuis()
		removeAllESP()
	end
end)

KillAllSection:AddButton({
	Title = "Kill All (ì´ë“¤ê³  ì‹¤í–‰í•˜ì„¸ìš”)",
	Callback = function()
		if processing then return end
		local tool = getTool()
		if not tool then Fluent:Notify({Title="ì˜¤ë¥˜", Content="ì´ì„ ë“œì„¸ìš”"}) return end

		local userid = GetPlayerUserId(LocalPlayer)
		local retrieved = filtergc("table", { Keys = { "remote_key" } }, true)
		if retrieved and retrieved.remote_key then
			for _, v in pairs(retrieved.remote_key) do remote_key[userid] = v break end
		end

		if not remote_key[userid] then Fluent:Notify({Title="ì˜¤ë¥˜", Content="í‚¤ë¥¼ ì°¾ì§€ ëª»í•¨."}) return end

		task.spawn(function()
			processing = true
			for i = 1, 60 do
				local targets = {}
				for _, p in pairs(Players:GetPlayers()) do if isTargetValid(p) then table.insert(targets, p) end end
				if #targets == 0 then break end
				for _, target in pairs(targets) do
					local k = NextRemoteKey(LocalPlayer)
					if k then ByPassKey(k, target) end
					task.wait(0.05)
				end
				task.wait(0.1)
			end
			processing = false
		end)
	end
})

Misc:AddButton({ Title = "Infinite Yield", Callback = function() loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))() end })
local BypassArrest = Misc:AddToggle("ì˜ì°½ìš°íšŒ", { Title = "ì˜ì°½ìš°íšŒ", Default = false })
local IsBypassArrest = false
local BypassConn

BypassArrest:OnChanged(function()
	task.spawn(function()
		IsBypassArrest = BypassArrest.Value
		if IsBypassArrest then
			local PlaceId, JobId = game.PlaceId, game.JobId
			local TeleportService = game:GetService("TeleportService")

			local function Rejoin()
				LocalPlayer:Kick("ì˜ì°½ìš°íšŒ ìž‘ë™")
			end

			local function OnChat(plr)
				plr.Chatted:Connect(function(msg)
					if not IsBypassArrest then return end
					local command, targetName = msg:match("^[:;]([^%s]+)%s+([^%s]+)")
					if command and targetName and (command == "ì˜ì°½" or command == "arrest" or command == "ì²´í¬") then
						local myName = LocalPlayer.Name:lower()
						local tn = targetName:lower()
						if tn == myName or tn == myName:sub(1, #tn) then
							Rejoin()
						end
					end
				end)
			end

			for _, plr in pairs(Players:GetPlayers()) do
				OnChat(plr)
			end
			BypassConn = Players.PlayerAdded:Connect(OnChat)
		else
			if BypassConn then
				BypassConn:Disconnect()
				BypassConn = nil
			end
		end
	end)
end)


local mt = getrawmetatable(game)
local old = mt.__index
setreadonly(mt, false)

mt.__index = newcclosure(function(self, key)
	if not checkcaller() and self:IsA("Mouse") and key == "Hit" then
		if SilentAim.Value and CurrentTarget then
			local char = LocalPlayer.Character
			local humanoid = char and char:FindFirstChild("Humanoid")

			if humanoid then
				if humanoid.WalkSpeed > 16 then
					local prediction = CurrentTarget.Velocity * 0.0625
					return CFrame.new(CurrentTarget.Position - prediction)
				else
					return CFrame.new(CurrentTarget.Position)
				end
			end
		end
	end
	return old(self, key)
end)

setreadonly(mt, true)

